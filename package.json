{
  "name": "phone-book",
  "version": "1.0.0",
  "description": "phone book",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "migration:add": "npx sequelize-cli migration:create",
    "migration:up": "npx sequelize-cli db:migrate",
    "migration:down": "npx sequelize-cli db:migrate:undo",
    "lint": "eslint 'src/**/*.{js,jsx}'",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write .",
    "test:unit": "jest test/unit/**.test.js --no-cache  --detectOpenHandles --runInBand --forceExit --silent",
    "test:integration": "jest test/integration/**.test.js --no-cache  --detectOpenHandles --runInBand --forceExit --silent",
    "test": "npm run test:unit && npm run test:integration",
    "prepare": "husky install"
  },
  "keywords": [
    "catalog",
    "service",
    "microservice",
    "expressjs",
    "nodejs"
  ],
  "author": "Hakan Keten",
  "license": "MIT",
  "dependencies": {
    "@sentry/node": "^6.17.6",
    "@sentry/tracing": "^6.17.6",
    "axios": "^0.25.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.0",
    "express": "4.17.2",
    "express-jsdoc-swagger": "^1.6.7",
    "express-validator": "^6.14.0",
    "helmet": "^5.0.2",
    "i18next": "^21.8.4",
    "i18next-fs-backend": "^1.1.4",
    "i18next-http-middleware": "^3.2.0",
    "ioredis": "^4.28.5",
    "json-to-pretty-yaml": "^1.2.2",
    "jsonwebtoken": "^8.5.1",
    "kafkajs": "^1.16.0",
    "lodash": "^4.17.21",
    "moment-timezone": "^0.5.34",
    "mysql2": "^2.3.3",
    "pg": "^8.7.3",
    "sequelize": "^6.16.1",
    "umzug": "^3.0.0",
    "uuid": "^8.3.2",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "eslint": "8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^17.0.0-1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "6.0.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "prettier": "2.5.1"
  }
}
